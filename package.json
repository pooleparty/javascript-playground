{
  "name": "node-webapp-starter-kit",
  "version": "1.0.0",
  "description": "Personal starter kit for javascript projects",
  "main": "index.js",
  "scripts": {
    "postinstall": "npm rebuild node-sass",
    "start": "node ./bin/server",
    "clean": "run-p -cl clean:client clean:server",
    "clean:client": "rimraf ./bin",
    "clean:server": "rimraf ./public/bundles",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest src/",
    "test:watch": "jest src/ --watch",
    "prebuild": "npm run clean",
    "build": "run-p -cl build:server build:client",
    "build:client": "BABEL_ENV=client webpack -p --progress --colors --env=dist",
    "build:server": "BABEL_ENV=node babel src/server/ -s -d bin",
    "dev:server": "BABEL_ENV=node nodemon $NODE_DEBUG_OPTION ./src/server/devServer.js --config nodemon.config.json",
    "dev:server:debug": "NODE_DEBUG_OPTION='--inspect' npm run dev:server",
    "predev:client": "npm run build:server",
    "dev:client": "BABEL_ENV=client webpack --watch",
    "dev:style": "npm run build:style -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pooleparty/node-webapp-starter-kit.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pooleparty/node-webapp-starter-kit/issues"
  },
  "homepage": "https://github.com/pooleparty/node-webapp-starter-kit#readme",
  "dependencies": {
    "body-parser": "^1.18.2",
    "classnames": "^2.2.5",
    "cookie-parser": "^1.4.3",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "express-correlation-id": "^1.0.2",
    "express-winston": "^2.4.0",
    "http-status": "^1.0.1",
    "lodash": "^4.17.4",
    "prop-types": "^15.6.0",
    "pug": "^2.0.0-rc.4",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "serve-favicon": "^2.4.5",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-require-ignore": "^0.1.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "css-loader": "^0.28.7",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "eslint": "^4.10.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "jest": "^21.2.1",
    "node-sass": "^4.5.3",
    "nodemon": "^1.12.1",
    "npm-run-all": "^4.1.1",
    "postcss-loader": "^2.0.8",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "stylelint": "^8.2.0",
    "stylelint-config-standard": "^17.0.0",
    "webpack": "^3.8.1",
    "webpack-merge": "^4.1.0"
  }
}
